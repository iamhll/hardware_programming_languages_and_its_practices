   "source": [
    "import struct\n",
    "def hex2double(strHex):\n",
    "    temp = bytes.fromhex(strHex)\n",
    "    datDouble = struct.unpack('>d', temp)[0]\n",
    "    return datDouble\n",
    "\n",
    "strHex = '4127e2c81f14ae90'\n",
    "datDouble = hex2double(strHex)\n",
    "print(f\"十六进制'{strHex}'对应的双精度浮点数是:{datDouble}\")"
   ]
  }
 ],
